    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-source-filesystem": "2.10.0",
    "node-sass": "4.14.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "keywords": [
    "react",
    "mdx",
    "gatsby"
