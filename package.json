{
  "name": "hello-gatsby-mdx",
  "description": "A bare-bones MDX starter for Gatsby",
  "version": "0.1.2",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
